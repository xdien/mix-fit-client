    "version": "0.2.0",
    "configurations": [
        {
            "name": "Mix Fit (Development)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=ENV=development",
                "--dart-define=BASE_URL=http://localhost:3000"
            ]
        },
        {
            "name": "Mix Fit (Production)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=ENV=production"
            ]
        },
        {
            "name": "Mix Fit (Profile Mode - Development)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=ENV=development"
            ]
        },
        {
            "name": "Mix Fit (Profile Mode - Production)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=ENV=production"
            ]
        },
        {
            "name": "Mix Fit (Release Mode - Development)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=ENV=development"
            ]
        },
        {
            "name": "mix-fit-client (Release Mode - Production)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=ENV=production"
            ]
        },
        {
            "name": "apis",
            "cwd": "lib/data/network/apis",
            "request": "launch",
            "type": "dart"
        }
    ]
}